<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/profile_twitter"
        android:layout_width="78dp"
        android:layout_height="67dp"
        app:srcCompat="@drawable/sonia_marnez___moda__belleza__lifestyle__diy__sobres_handmade_para_las_notas_de_agradecimiento_en_la_boda"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/text_twit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/aclonica"
        android:text="Twitter"
        app:layout_constraintTop_toBottomOf="@+id/profile_twitter"
        app:layout_constraintEnd_toEndOf="@+id/profile_twitter"
        app:layout_constraintStart_toStartOf="@+id/profile_twitter" />
</androidx.constraintlayout.widget.ConstraintLayout>